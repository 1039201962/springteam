<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.st.springsys.sysmenu.dao.SysMenuDao">
	<insert id="insertObject"
		parameterType="com.st.springsys.sysmenu.entity.SysMenu">
		insert into menus
		(name,parent_id,permission)
		values
		(#{name},#{parent_id},#{permission})
	</insert>

	<update id="updateObject"
		parameterType="com.st.springsys.sysmenu.entity.SysMenu">
		update menus
		set
		name=#{name},
		parent_id=#{parent_id},
		permission=#{permission}
		where id=#{id}
	</update>


</mapper>